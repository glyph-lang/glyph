// Demonstrates tuple returns and mutable variables for parsing

fn parse_value(pos: i32) -> (bool, i32) {
    let new_pos = pos + 5
    ret (true, new_pos)
}

fn main() {
    println("=== JSON Parser Demo ===")
    println("")
    
    // Test mutable position tracking
    println("Parsing sequence:")
    let mut p = 0
    
    let r1 = parse_value(p)
    p = r1.1
    println("  Parsed value at position 5")
    
    let r2 = parse_value(p)
    p = r2.1
    println("  Parsed value at position 10")
    
    let r3 = parse_value(p)
    p = r3.1
    println("  Parsed value at position 15")
    
    println("")
    println("All values parsed successfully!")
    println("Features demonstrated:")
    println("  - Tuple return types: (bool, i32)")
    println("  - Mutable variables: let mut p")
    println("  - Sequential parsing pattern")
}
