fn make_pair() -> (i32, i32) {
  ret (1, 2)
}

fn make_nested() -> ((i32, i32), i32) {
  ret ((3, 4), 5)
}

fn main() -> i32 {
  let pair = make_pair()
  if pair.0 != 1 { ret 1 }
  if pair.1 != 2 { ret 2 }

  let nested = make_nested()
  if nested.0.0 != 3 { ret 3 }
  if nested.0.1 != 4 { ret 4 }
  if nested.1 != 5 { ret 5 }

  ret 0
}
